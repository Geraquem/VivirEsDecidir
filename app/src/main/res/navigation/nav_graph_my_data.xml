<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_my_data"
    app:startDestination="@id/myDataFragment">

    <fragment
        android:id="@+id/myDataFragment"
        android:name="com.mmfsin.quepreferirias.presentation.myideas.viewpager.MyIdeasViewPagerFragment"
        android:label="myDataFragment"
        tools:layout="@layout/fragment_viewpager">
        <action
            android:id="@+id/myIdeas_to_single_Dilemma"
            app:destination="@id/nav_graph_single_dilemma">
            <argument
                android:name="dilemmaId"
                app:argType="string" />
        </action>
    </fragment>

    <include app:graph="@navigation/nav_graph_single_dilemma" />

</navigation>