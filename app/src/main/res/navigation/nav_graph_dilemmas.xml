<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_dilemmas"
    app:startDestination="@id/dilemmasFragment">

    <fragment
        android:id="@+id/dilemmasFragment"
        android:name="com.mmfsin.quepreferirias.presentation.dashboard.dilemmas.DilemmasFragment"
        android:label="dilemmasFragment"
        tools:layout="@layout/fragment_dilemma">
        <action
            android:id="@+id/action_to_userProfile"
            app:destination="@id/nav_graph_other_profile">
            <argument
                android:name="userId"
                app:argType="string" />
        </action>
    </fragment>

    <include app:graph="@navigation/nav_graph_other_profile" />

</navigation>