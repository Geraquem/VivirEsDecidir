<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_profile"
    app:startDestination="@id/profileFragment">

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.mmfsin.quepreferirias.presentation.profile.myprofile.ProfileFragment"
        android:label="profileFragment"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_to_myData"
            app:destination="@id/nav_graph_my_data" />
        <action
            android:id="@+id/action_to_savedData"
            app:destination="@id/nav_graph_saved_data" />
    </fragment>

    <include app:graph="@navigation/nav_graph_my_data" />

    <include app:graph="@navigation/nav_graph_saved_data" />

</navigation>